@model IEnumerable<Register>

@{
    ViewData["Title"] = "Account";
}

@if (User.Identity.IsAuthenticated)
                    {
<h1>User List</h1>

<hr />

<table class="table table-striped" id="userTable">
    <thead>
        <tr>
            @*<th>Id</th>*@
            <th>Email</th>
            <th>PhoneNumber</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                @*<td>@user.Id</td>*@
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
               @* <td>@string.Join(" , ", user.Roles.ToList())</td>*@
                <td>
                    @*<a class="btn btn-primary" asp-action="ManageRoles" asp-route-userId="@user.Id">Roles</a>*@
                       @* <a class="btn btn-primary" asp-action="Update" asp-route-userId="@user.Id">Update</a>
                        <a class="btn btn-danger" asp-action="Delete" asp-route-userId="@user.Id" method ="post">Delete</a>*@
                    <form asp-action="Delete" asp-route-id="@user.Id" method="post">
                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete
                    </button>
                    </form>
                </td>
               <td>
                <form asp-action="Update" asp-route-id="@user.Id" method="post">
                    <button type="submit" class="btn btn-sm btn-danger">
                                Update
                    </button>
                </form>
            </td>
            </tr>
        }
    </tbody>
</table>
}